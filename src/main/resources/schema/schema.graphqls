type Query {
    events: [Event!]!
}

type Mutation {
    createEvent(eventInput: EventInput!): Event!
}

input EventInput {
    title: String!
    description: String!
    price: Float!
    date: String!
}

type Event {
    id: ID!
    title: String!
    description: String!
    price: Float!
    date: String!
}